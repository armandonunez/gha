default_platform(:android)
fastlane_require 'dotenv'

platform :android do
  lane :demo do
    if "#{ENV['APP_SHA_REF']}".length > 0
      app_sha_ref = ENV["APP_SHA_REF"];
    end

    puts("SOMETHING - #{app_sha_ref}");
  end

  lane :undemo do
    app_sha_ref = "SOMETHING - #{ENV['APP_SHA_REF'].length ? ENV['APP_SHA_REF'] : ''} - END";

    puts("Something - #{app_sha_ref}");
  end
end
